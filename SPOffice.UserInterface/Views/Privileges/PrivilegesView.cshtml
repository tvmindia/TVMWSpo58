@model UserInterface.Models.PrivilegesViewModel
@using UserInterface.App_Start
@{
    ViewBag.Title = "View Privileges";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<div style="width:100%;">
    <ol class="breadcrumb">
        <li><i class="fa fa-cogs"></i> Settings</li>
        <li><a href=@Url.Content("~/SAMPanel")><i class="fa fa-users"></i> User management</a></li>
        <li class="active">@ViewBag.Title</li>
    </ol>
</div>


<div id="ButtonPatchDiv" class="col-md-12">
    <div class="" id="btnPatchManage" style="min-height:25px">

        @{
            var data = new { ActionType = "GoBack" };
            @Html.Action("ChangeButtonStyleForVP", "Privileges", data);
        }
    </div>

</div>




<div id="ManagePrivilegesTableDiv" class="col-md-12">
    <table id="tblPrivilegesList" class="table table-striped table-bordered table-hover" cellspacing="0">
        <thead>
            <tr class="text-center">
                <th>ID</th>
                <th>Application Name</th>
                <th>Role Name</th>
                <th>Module Name</th>
                <th>Access Description</th>
                <th>Created Date</th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/bundles/datatable")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/bundles/jqueryunobtrusiveajaxvalidate")
    @Scripts.RenderFormat(SiteKeys.ScriptVersion, "~/bundles/PrivilegesView")
}


@model SPOffice.UserInterface.Models.FollowUpListViewModel

<div class="col-md-12">
  


    <div class="box box-primary">
        <div class="box-header with-border">

            <div class="box-body">
               <input type ="hidden" id="hdnCountOpen" value=@ViewBag.Count />
                <ul class="products-list product-list-in-box">


                    <li class="item">
                        @if (Model.FollowUpList != null)
                        {

                            for (int i = 0; i < Model.FollowUpList.Count; i++)
                            {
                                <div class="col-md-8 ">
                                    <div class="col-md-6 text-left ">
                                        <b>Date</b>
                                    </div>
                                    <div class="col-md-6 text-left">
                                        @Model.FollowUpList[i].FollowUpDate
                                        Date
                                    </div>
                                </div>

                                <div class="col-md-8">
                                    <div class="col-md-6 text-left">
                                        <b>Time</b>
                                    </div>
                                    <div class="col-md-6 text-left">
                                        @Model.FollowUpList[i].FollowUpTime
                                    </div>
                                </div>
                                <div class="product-img">
                                    <img src="dist/img/default-50x50.gif" alt="Product Image">
                                </div>
                                <div class="product-info">
                                    <a href="#" class="product-title">
                                        Subject
                                        <span  class="label label-warning pull-right"   onclick="FollowUp(id)" id=@Model.FollowUpList[i].ID >@Model.FollowUpList[i].Status</span>
                                   
                                    <span class="label label-warning pull-right">@Model.FollowUpList[i].PriorityDescription</span></a>
                                    <span class="product-description">
                                       @Model.FollowUpList[i].Subject
                                    </span>
                                </div>
                                    @*<a href="#" onclick="FollowUp(id)" id=@Model.FollowUpList[i].ID>Edit</a>*@

                            }
                        }
                    </li>

                    <!-- /.item -->
                </ul>
            </div>


        </div>
    </div>




        </div>
